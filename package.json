{
    "name": "image-sketchpad",
    "version": "1.0.2",
    "description": "Draw on images within desktop and mobile browsers.",
    "keywords": [
        "image",
        "sketchpad",
        "responsive",
        "desktop",
        "mobile",
        "mark",
        "canvas",
        "html5"
    ],
    "homepage": "https://github.com/CSoellinger/image-sketchpad",
    "bugs": "https://github.com/CSoellinger/image-sketchpad/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/CSoellinger/image-sketchpad.git"
    },
    "license": "MIT",
    "author": {
        "name": "CSoellinger",
        "email": "christopher.soellinger@gmail.com",
        "url": "https://github.com/CSoellinger"
    },
    "contributors": [
        {
            "name": "CSoellinger",
            "email": "christopher.soellinger@gmail.com",
            "url": "https://github.com/CSoellinger"
        }
    ],
    "main": "dist/image-sketchpad.cjs.js",
    "module": "dist/image-sketchpad.esm.js",
    "browser": "dist/image-sketchpad.js",
    "types": "types/index.d.ts",
    "files": [
        "dist",
        "types"
    ],
    "scripts": {
        "prebuild": "npm run lint && npm run clean",
        "build": "npm run build:esnext & npm run build:js && npm run build:js:min",
        "prebuild:docs": "npm run lint && npm run clean:docs",
        "build:docs": "typedoc",
        "build:esnext": "tsc",
        "build:js": "rollup -c",
        "build:js:min": "terser --compress --mangle reserved=['ImageSketchpad'] --output ./dist/image-sketchpad.min.js ./dist/image-sketchpad.js",
        "build:types": "tsc --emitDeclarationOnly",
        "clean": "eliminate ./dist ./types",
        "clean:docs": "eliminate ./docs",
        "lint": "eslint --ext '.js,.ts' .",
        "lint:fix": "npm run lint -- --fix",
        "serve": "npm run watch & serve --no-port-switching .",
        "start": "babel-node -x \".ts\" src/index.ts",
        "test": "npm run test:unit",
        "test:e2e": "jest --config=./test/e2e/jest.config.js",
        "test:unit": "jest --config=./test/unit/jest.config.js",
        "test:unit:watch": "npm run test:unit -- --watchAll",
        "type-check": "tsc --noEmit",
        "prewatch": "npm run clean",
        "watch": "tsc -w & rollup -c -w",
        "watch:docs": "typedoc --watch",
        "watch:type-check": "npm run type-check -- --watch"
    },
    "dependencies": {
        "downloadjs": "^1.4.7",
        "draw-to-canvas": "^1.0.0",
        "merge-images": "^2.0.0",
        "pica": "^8.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/node": "^7.16.0",
        "@babel/plugin-proposal-class-properties": "^7.16.0",
        "@babel/plugin-transform-runtime": "^7.16.4",
        "@babel/preset-env": "^7.16.4",
        "@babel/preset-typescript": "^7.16.0",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-html": "^0.2.4",
        "@rollup/plugin-node-resolve": "^13.0.6",
        "@rollup/plugin-replace": "^3.0.0",
        "@rollup/plugin-strip": "^2.1.0",
        "@types/downloadjs": "^1.4.2",
        "@types/jest": "^27.0.3",
        "@types/jest-image-snapshot": "^4.3.1",
        "@types/merge-images": "^1.2.1",
        "@types/pica": "^5.1.3",
        "@typescript-eslint/eslint-plugin": "^5.5.0",
        "@typescript-eslint/parser": "^5.5.0",
        "canvas": "^2.8.0",
        "eliminate": "^1.1.2",
        "eslint": "^8.3.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-array-func": "^3.1.7",
        "eslint-plugin-editorconfig": "^3.2.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jest": "^25.3.0",
        "eslint-plugin-jest-formatting": "^3.1.0",
        "eslint-plugin-jsdoc": "^37.1.0",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-promise": "^5.2.0",
        "eslint-plugin-radar": "^0.2.1",
        "eslint-plugin-regexp": "^1.5.1",
        "eslint-plugin-tsdoc": "^0.2.14",
        "jest": "^27.4.3",
        "jest-canvas-mock": "^2.3.1",
        "jest-html-reporter": "^3.4.2",
        "jest-image-snapshot": "^4.5.1",
        "prettier": "^2.5.0",
        "prettier-plugin-packagejson": "^2.2.15",
        "remark-prettier": "^1.3.0",
        "rollup": "^2.60.2",
        "rollup-plugin-html2": "^3.1.0",
        "rollup-plugin-terser": "^7.0.2",
        "serve": "^13.0.2",
        "ts-jest": "^27.0.7",
        "tslib": "^2.3.1",
        "typedoc": "^0.22.10",
        "typedoc-plugin-missing-exports": "^0.22.6",
        "typescript": "^4.5.2"
    },
    "publishConfig": {
        "access": "public"
    },
    "esnext": "dist/esnext/index.js",
    "readme": "README.md"
}
